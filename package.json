{
  "name": "lv-app-frontend",
  "version": "0.0.1",
  "engines": {
    "npm": ">=3"
  },
  "scripts": {
    "preinstall": "node tools/nodeVersionCheck.js",
    "start-message": "node tools/startMessage.js",
    "prestart": "npm-run-all --parallel start-message remove-dist",
    "start": "npm-run-all --parallel test:watch open:src lint:watch",
    "open:src": "node tools/srcServer.js",
    "open:dist": "node tools/distServer.js",
    "lint": "esw --ext .js --ext .jsx --color src tools",
    "lint:watch": "esw -w --ext .js --ext .jsx --color src tools",
    "clean-dist": "npm run remove-dist && mkdir dist",
    "remove-dist": "rimraf ./dist",
    "prebuild": "npm run clean-dist && npm run lint && npm run test",
    "build": "node tools/build.js && npm run open:dist",
    "test": "mocha tools/testSetup.js \"./{,!(node_modules)/**/}*.spec.js\" --reporter progress",
    "test:watch": "npm run test -- --watch"
  },
  "dependencies": {
    "axios": "0.15.3",
    "bootstrap": "3.3.7",
    "jquery": "3.1.1",
    "react": "15.4.2",
    "react-bootstrap": "0.30.7",
    "react-dom": "15.4.2",
    "react-redux": "5.0.3",
    "react-router": "3.0.2",
    "react-router-redux": "4.0.8",
    "redux": "3.6.0",
    "redux-thunk": "2.2.0"
  },
  "devDependencies": {
    "autoprefixer": "6.7.6",
    "babel-core": "6.23.1",
    "babel-eslint": "7.1.1",
    "babel-loader": "6.3.2",
    "babel-plugin-transform-react-jsx-source": "6.22.0",
    "babel-preset-es2015": "6.22.0",
    "babel-preset-react": "6.23.0",
    "babel-preset-react-optimize": "1.0.1",
    "babel-preset-stage-0": "6.22.0",
    "babel-preset-stage-1": "6.22.0",
    "babel-register": "6.23.0",
    "chai": "3.5.0",
    "chalk": "1.1.3",
    "connect-history-api-fallback": "1.3.0",
    "cross-env": "3.1.4",
    "css-loader": "0.26.2",
    "enzyme": "2.7.1",
    "eslint": "3.17.0",
    "eslint-import-resolver-webpack": "0.8.1",
    "eslint-plugin-babel": "4.1.0",
    "eslint-plugin-import": "2.2.0",
    "eslint-plugin-react": "6.10.0",
    "eslint-watch": "3.0.0",
    "file-loader": "0.10.1",
    "html-webpack-plugin": "2.28.0",
    "immutable": "3.8.1",
    "json-loader": "0.5.4",
    "mocha": "3.2.0",
    "node-sass": "4.5.0",
    "normalizr": "3.2.2",
    "npm-run-all": "4.0.2",
    "postcss-loader": "1.3.3",
    "react-addons-test-utils": "15.4.2",
    "react-hot-loader": "3.0.0-beta.6",
    "redux-immutable-state-invariant": "1.2.4",
    "rimraf": "2.6.1",
    "sass-loader": "6.0.2",
    "sinon": "1.17.7",
    "sinon-chai": "2.8.0",
    "style-loader": "0.13.2",
    "url-loader": "0.5.8",
    "webpack": "1.14.0",
    "webpack-dev-server": "1.16.3"
  },
  "config": {
    "host": "localhost",
    "port": 3000
  },
  "repository": {
    "type": "git",
    "url": "https://KienDangTran@bitbucket.org/KienDangTran/lv-app-fronend.git"
  }
}
